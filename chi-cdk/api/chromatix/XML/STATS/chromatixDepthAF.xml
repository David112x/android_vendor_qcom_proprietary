<chromatixDepthAF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="..\..\xsd\stats\af\afdepthafelements.xsd">
  <module_version major_revision="8" minor_revision="0" incr_revision="0"/>

  <parameter_version>
    <revision>A</revision>
  </parameter_version>
  <stats>
    <minimalDistance>100</minimalDistance>
    <maximumDistance>3000</maximumDistance>
    <fovMap>
      <horizontalOffset>0.0</horizontalOffset>
      <verticalOffset>0.0</verticalOffset>
      <horizontalCoverage>1.0</horizontalCoverage>
      <verticalCoverage>1.0</verticalCoverage>
    </fovMap>
    <confidenceTable>
      <entries>
        <startLuxIndex>0</startLuxIndex>
        <endLuxIndex>100</endLuxIndex>
        <threshold>50</threshold>
      </entries>
      <entries>
        <startLuxIndex>200</startLuxIndex>
        <endLuxIndex>300</endLuxIndex>
        <threshold>40</threshold>
      </entries>
      <entries>
        <startLuxIndex>400</startLuxIndex>
        <endLuxIndex>500</endLuxIndex>
        <threshold>20</threshold>
      </entries>
    </confidenceTable>
    <center>
      <enable>true</enable>
      <horizontalGridCount>5</horizontalGridCount>
      <verticalGridCount>5</verticalGridCount>
      <horizontalSkip>1</horizontalSkip>
      <verticalSkip>1</verticalSkip>
      <minConfidentPixelPercentage>80</minConfidentPixelPercentage>
      <minConfidentGridPercentage>20</minConfidentGridPercentage>
      <defocusPercentile>0.0</defocusPercentile>
    </center>
    <peripheral>
      <enable>false</enable>
      <horizontalGridCount>1</horizontalGridCount>
      <verticalGridCount>1</verticalGridCount>
      <horizontalSkip>7</horizontalSkip>
      <verticalSkip>7</verticalSkip>
      <minConfidentPixelPercentage>80</minConfidentPixelPercentage>
      <minConfidentGridPercentage>20</minConfidentGridPercentage>
      <defocusPercentile>0.0</defocusPercentile>
    </peripheral>
    <touch>
      <enable>true</enable>
      <horizontalGridCount>1</horizontalGridCount>
      <verticalGridCount>1</verticalGridCount>
      <horizontalSkip>1</horizontalSkip>
      <verticalSkip>1</verticalSkip>
      <minConfidentPixelPercentage>80</minConfidentPixelPercentage>
      <minConfidentGridPercentage>20</minConfidentGridPercentage>
      <defocusPercentile>0.0</defocusPercentile>
    </touch>
    <face>
      <enable>true</enable>
      <horizontalGridCount>1</horizontalGridCount>
      <verticalGridCount>1</verticalGridCount>
      <horizontalSkip>1</horizontalSkip>
      <verticalSkip>1</verticalSkip>
      <minConfidentPixelPercentage>80</minConfidentPixelPercentage>
      <minConfidentGridPercentage>20</minConfidentGridPercentage>
      <defocusPercentile>0.0</defocusPercentile>
    </face>
  </stats>
  <monitor>
    <DOFScalerTable>
      <entries>
        <normalizedLensPosition>0.0</normalizedLensPosition>
        <DOFScaler>2.0</DOFScaler>
