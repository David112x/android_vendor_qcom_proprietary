        <endPosition>50</endPosition>
        <focusDoneThreshold>10</focusDoneThreshold>
        <fineScanStepSize>10</fineScanStepSize>
        <stepsRangeNear>3</stepsRangeNear>
        <stepsRangeFar>3</stepsRangeFar>
      </entries>
      <entries>
        <startPosition>150</startPosition>
        <endPosition>250</endPosition>
        <focusDoneThreshold>8</focusDoneThreshold>
        <fineScanStepSize>8</fineScanStepSize>
        <stepsRangeNear>2</stepsRangeNear>
        <stepsRangeFar>2</stepsRangeFar>
      </entries>
      <entries>
        <startPosition>300</startPosition>
        <endPosition>399</endPosition>
        <focusDoneThreshold>8</focusDoneThreshold>
        <fineScanStepSize>8</fineScanStepSize>
        <stepsRangeNear>0</stepsRangeNear>
        <stepsRangeFar>0</stepsRangeFar>
