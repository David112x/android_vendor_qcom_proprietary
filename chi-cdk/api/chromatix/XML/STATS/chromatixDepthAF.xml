      <entries>
        <normalizedLensPosition>1.0</normalizedLensPosition>
        <DOFScaler>1.0</DOFScaler>
      </entries>
    </DOFScalerTable>
  </monitor>
  <converge>
    <waitConfidenceRecoverCount>5</waitConfidenceRecoverCount>
    <frameSkip>0</frameSkip>
    <normalLight>
      <entries>
        <startPosition>0</startPosition>
        <endPosition>50</endPosition>
        <focusDoneThreshold>12</focusDoneThreshold>
        <fineScanStepSize>10</fineScanStepSize>
        <stepsRangeNear>4</stepsRangeNear>
        <stepsRangeFar>4</stepsRangeFar>
