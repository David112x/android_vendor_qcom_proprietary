            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="c4">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:float">
              <xs:attribute name="type" default="float"/>
              <xs:attribute name="description" type="xs:string" default="weights of R/G/B/MAX(R,G,B) contribute to Y"/>
              <xs:attribute name="default_value" type="xs:string" default="0.5"/>
