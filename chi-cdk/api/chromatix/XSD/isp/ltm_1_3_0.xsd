              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="c3">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:float">
              <xs:attribute name="type" default="float"/>
              <xs:attribute name="description" type="xs:string" default="weights of R/G/B/MAX(R,G,B) contribute to Y"/>
              <xs:attribute name="default_value" type="xs:string" default="0.125"/>
              <xs:attribute name="range" type="xs:string" default="[0.0,1.0]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="0uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="Q6"/>
