  <xs:complexType name="video10_rgn_dataType">
    <xs:sequence>
      <xs:element name="general">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="max_vsr">
              <xs:complexType>
                <xs:simpleContent>
