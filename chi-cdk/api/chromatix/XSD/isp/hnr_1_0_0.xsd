<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="ispglobalelements.xsd"/>
  <xs:element name="hnr10_bps" type="chromatix_hnr10Type">
    <xs:annotation>
      <xs:documentation>hnr10 bps module parameters</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="hnr10_ipe" type="chromatix_hnr10Type">
    <xs:annotation>
      <xs:documentation>hnr10 ipe module parameters</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="chromatix_hnr10Type">
    <xs:sequence>
      <xs:element name="enable_section">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="hnr_nr_enable">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="enable_flag_type">
                    <xs:attribute name="type" default="int"/>
                    <xs:attribute name="description" type="xs:string" default="HNR noise reduction enable bit"/>
                    <xs:attribute name="default_value" type="xs:string" default="1"/>
                    <xs:attribute name="range" type="xs:string" default="[0,1]"/>
                    <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
                    <xs:attribute name="length" type="xs:int" fixed="1"/>
                    <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
                    <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                    <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="hnr_blend_enable">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="enable_flag_type">
                    <xs:attribute name="type" default="int"/>
                    <xs:attribute name="description" type="xs:string" default="HNR blending enable bit"/>
                    <xs:attribute name="default_value" type="xs:string" default="1"/>
                    <xs:attribute name="range" type="xs:string" default="[0,1]"/>
                    <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
                    <xs:attribute name="length" type="xs:int" fixed="1"/>
                    <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
                    <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                    <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="module_version">
        <xs:complexType>
          <xs:attribute name="major_revision" type="xs:int" fixed="1"/>
          <xs:attribute name="minor_revision" type="xs:int" fixed="0"/>
          <xs:attribute name="incr_revision" type="xs:int" fixed="0"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="parameter_version">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="revision" type="xs:string" default="0"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="control_variables">
        <xs:complexType>
          <xs:sequence minOccurs="4" maxOccurs="4">
            <xs:element name="control_var_type">
              <xs:simpleType>
                <xs:restriction base="control_var_type">
                  <xs:pattern value="control_total_scale_ratio"/>
                  <xs:pattern value="control_drc_gain"/>
                  <xs:pattern value="control_exp_time_ratio"/>
                  <xs:pattern value="control_aec_exp_sensitivity_ratio"/>
                  <xs:pattern value="control_exp_gain_ratio"/>
                  <xs:pattern value="control_lux_idx"/>
                  <xs:pattern value="control_gain"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="default_value" type="xs:string" default="control_total_scale_ratio, control_drc_gain, control_aec_exp_sensitivity_ratio, control_gain"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="control_method">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="aec_exp_control" type="tuning_control_aec_type" default="control_gain"/>
            <xs:element name="aec_hdr_control" type="tuning_control_aec_hdr_type" default="control_aec_exp_sensitivity_ratio"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="dynamic_enable_triggers">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="hnr_nr_enable">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="enable">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="enable_flag_type">
                          <xs:attribute name="type" default="int"/>
                          <xs:attribute name="description" type="xs:string" default="enable HNR1.0 module"/>
                          <xs:attribute name="default_value" type="xs:string" default="0"/>
                          <xs:attribute name="range" type="xs:string" default="[0,1]"/>
                          <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
                          <xs:attribute name="length" type="xs:int" fixed="1"/>
                          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
                          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="hyst_control_var">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="control_var_type">
                          <xs:attribute name="type" default="uint"/>
                          <xs:attribute name="description" type="xs:string" default="enable HNR1.0 module"/>
                          <xs:attribute name="default_value" type="xs:string" default="control_gain"/>
                          <xs:attribute name="range" type="xs:string" default="[0,1]"/>
                          <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
                          <xs:attribute name="length" type="xs:int" fixed="1"/>
                          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
                          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="hyst_mode" type="hyst_direction" default="UPWARD"/>
                  <xs:element name="hyst_trigger" type="trigger_pt_couplet_type"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="chromatix_hnr10_reserve" type="chromatix_hnr10_reserveType"/>
      <xs:element name="chromatix_hnr10_core" type="chromatix_hnr10_coreType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="chromatix_hnr10_reserveType">
    <xs:sequence>
      <xs:element name="lnr_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of luma based noise reduction"/>
              <xs:attribute name="default_value" type="xs:string" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="rnr_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of radius based noise reduction"/>
              <xs:attribute name="default_value" type="xs:string" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="cnr_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of chroma based noise reduction"/>
              <xs:attribute name="default_value" type="xs:string" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="snr_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of skin color based noise reduction"/>
              <xs:attribute name="default_value" type="xs:string" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="fd_snr_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of face guided skin color based noise reduction"/>
              <xs:attribute name="default_value" type="xs:string" default="0"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="if fd_snr_en = 1, snr is applied on face regions; if fd_snr_en  = 0, snr is applied on both face regions and non-face regions"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="fnr_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of flatness based noise reduction"/>
              <xs:attribute name="default_value" type="xs:string" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="lpf3_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of low-pass filter"/>
              <xs:attribute name="default_value" type="xs:string" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="blend_cnr_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of color based noise blending"/>
              <xs:attribute name="default_value" type="xs:string" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="blend_snr_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of skin color based blending"/>
              <xs:attribute name="default_value" type="xs:string" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="radial_anchor_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="radial_anchor">
              <xs:simpleType>
                <xs:restriction base="floatvar">
                  <xs:length value="7"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="float"/>
          <xs:attribute name="description" type="xs:string" default="radial anchor table (distance percentage)"/>
          <xs:attribute name="default_value" type="xs:string" default="0.0 0.2 0.3 0.4 0.6 0.8 1.0"/>
          <xs:attribute name="range" type="xs:string" default="[0.0,1.0]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="10uQ0"/>
          <xs:attribute name="length" type="xs:int" fixed="7"/>
          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="chromatix_hnr10_coreType">
    <xs:sequence maxOccurs="unbounded">
      <xs:element name="mod_hnr10_total_scale_ratio_data" type="mod_hnr10_total_scale_ratio_dataType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_hnr10_total_scale_ratio_dataType">
    <xs:sequence>
      <xs:element name="total_scale_ratio_trigger" type="trigger_pt_type"/>
      <xs:element name="total_scale_ratio_data">
        <xs:complexType>
          <xs:sequence maxOccurs="unbounded">
            <xs:element name="mod_hnr10_drc_gain_data" type="mod_hnr10_drc_gain_dataType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_hnr10_drc_gain_dataType">
    <xs:sequence>
      <xs:element name="drc_gain_trigger" type="trigger_pt_type"/>
      <xs:element name="drc_gain_data">
        <xs:complexType>
          <xs:sequence maxOccurs="unbounded">
            <xs:element name="mod_hnr10_hdr_aec_data" type="mod_hnr10_hdr_aec_dataType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_hnr10_hdr_aec_dataType">
    <xs:sequence>
      <xs:element name="hdr_aec_trigger" type="trigger_pt_type_aec_hdr"/>
      <xs:element name="hdr_aec_data">
        <xs:complexType>
          <xs:sequence maxOccurs="unbounded">
            <xs:element name="mod_hnr10_aec_data" type="mod_hnr10_aec_dataType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_hnr10_aec_dataType">
    <xs:sequence>
      <xs:element name="aec_trigger" type="trigger_pt_type_aec"/>
      <xs:element name="hnr10_rgn_data" type="hnr10_rgn_dataType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="hnr10_rgn_dataType">
    <xs:sequence>
      <xs:element name="lnr_gain_arr_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="lnr_gain_arr">
              <xs:simpleType>
                <xs:restriction base="uintvar">
                  <xs:length value="33"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="uint"/>
          <xs:attribute name="description" type="xs:string" default="luma based noise reduction gain table"/>
          <xs:attribute name="default_value" type="xs:string" default="32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32"/>
          <xs:attribute name="range" type="xs:string" default="[0,63]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="6uQ5"/>
          <xs:attribute name="length" type="xs:int" fixed="33"/>
          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="HNR will be stronger as lnr_gain_arr decreases."/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="lnr_shift">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:unsignedInt">
              <xs:attribute name="type" default="uint"/>
              <xs:attribute name="description" type="xs:string" default="LNR right shift used to adjust LNR LUT input value"/>
              <xs:attribute name="default_value" type="xs:string" default="2"/>
              <xs:attribute name="range" type="xs:string" default="[0,3]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="2uQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="HNR will be stronger as lnr_shift increases."/>
              <xs:attribute name="tool_hint" default="float"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="radial_noise_prsv_adj_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="radial_noise_prsv_adj">
              <xs:simpleType>
                <xs:restriction base="floatvar">
                  <xs:length value="7"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="float"/>
          <xs:attribute name="description" type="xs:string" default="radial adjustment for noise preserving percentage"/>
          <xs:attribute name="default_value" type="xs:string" default="1.0 1.0 0.9375 0.8750 0.7500 0.6250 0.5000"/>
          <xs:attribute name="range" type="xs:string" default="[0,1.0]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="10uQ9"/>
          <xs:attribute name="length" type="xs:int" fixed="7"/>
          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="HNR will be stronger denoising as radial_noise_prsv_adj decreases. In general, radial_noise_prsv_adj is a monotonically decreasing curve."/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="cnr_gain_arr_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="cnr_gain_arr">
              <xs:simpleType>
                <xs:restriction base="uintvar">
                  <xs:length value="25"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="uint"/>
          <xs:attribute name="description" type="xs:string" default="chroma based noise reduction(distance percentage) for peak noise reduction"/>
          <xs:attribute name="default_value" type="xs:string" default="32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32 32"/>
          <xs:attribute name="range" type="xs:string" default="[0,63]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="6uQ5"/>
          <xs:attribute name="length" type="xs:int" fixed="25"/>
          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="HNR will be stronger as cnr_gain_arr decreases."/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="cnr_low_thrd_u">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:int">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="CNR low-bound threshold for U channel"/>
              <xs:attribute name="default_value" type="xs:int" default="2"/>
              <xs:attribute name="range" type="xs:string" default="[-128,127]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="8sQ0"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
              <xs:attribute name="tool_hint" default="float"/>
            </xs:extension>
          </xs:simpleContent>
